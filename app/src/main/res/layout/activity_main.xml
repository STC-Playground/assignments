<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/toolbar"
    tools:context=".MainActivity">>


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/polynesian_blue"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:title="Setup Mode"
            app:navigationIcon="@drawable/baseline_arrow_back_24"
            app:titleTextColor="@color/white" />

        <androidx.cardview.widget.CardView
            android:id="@+id/barCodeContainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/toolbar"
            app:cardCornerRadius="5dp">

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/barcodeContentHeader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    >

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/polynesian_blue"
                        android:drawableLeft="@drawable/baseline_chat_24"
                        android:drawablePadding="8dp"
                        android:padding="12dp"
                        android:layout_weight=".6"
                        android:text="Barcode Content"
                        android:textColor="@color/white"
                        android:textSize="16sp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="Model"
                        android:background="@color/polynesian_blue"
                        android:textColor="@color/white"
                        android:gravity="center_vertical"
                        android:textSize="12sp"
                        android:padding="12dp"
                        android:drawableRight="@drawable/baseline_branding_watermark_24"
                        android:drawablePadding="8dp" />

                </LinearLayout>

                <EditText
                    android:id="@+id/getBarcode"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="8dp"
                    android:layout_marginVertical="8dp"
                    android:textSize="16sp"
                    android:backgroundTint="@color/cadet_gray"
                    />

            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/barCodeStandardContainer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/barCodeContainer"
            app:cardCornerRadius="5dp">

            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/barcodeStandardHeader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="H/KMC Part 2D Barcode Standard"
                    android:background="@color/polynesian_blue"
                    android:textColor="@color/white"
                    android:textSize="18sp"
                    android:padding="12dp"
                    android:drawableLeft="@drawable/baseline_chat_24"
                    android:drawablePadding="8dp" />

                <TableLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text="Item"
                            android:paddingVertical="8dp"
                            android:layout_weight="2"
                            android:background="@drawable/cell_seasalt_bg"/>
                        <TextView
                            android:textAlignment="center"
                            android:layout_height="match_parent"
                            android:layout_width="wrap_content"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text="Result"
                            android:paddingVertical="8dp"
                            android:layout_weight="0.5"
                            android:background="@drawable/cell_seasalt_bg"/>
                        <TextView
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text="Data"
                            android:layout_weight="3"
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_seasalt_bg"/>

                    </TableRow>
                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:layout_weight="2"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text="Header"
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_seasalt_bg"/>
                        <TextView
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text=""
                            android:layout_weight=".5"
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_white_bg"/>

                        <TextView
                            android:layout_height="match_parent"
                            android:textAlignment="center"
                            android:layout_weight="3"
                            android:textSize="14sp"
                            android:textStyle="bold"
                            android:text=""
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_white_bg"/>

                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_weight="2">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/cell_seasalt_bg"
                                android:gravity="center"
                                android:text="Spec"
                                android:layout_weight="1"
                                android:textAlignment="center"
                                android:textStyle="bold" />
                            <TableLayout
                                android:layout_height="match_parent"
                                android:layout_width="wrap_content">
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:lines="2"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:text="Supplier\nCode"
                                        android:paddingVertical="4dp" />
                                </TableRow>

                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:paddingVertical="4dp"
                                        android:text="Part\nNumber" />
                                </TableRow>
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:text="Sequence\nCode"
                                        android:paddingVertical="4dp"/>
                                </TableRow>
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:paddingVertical="4dp"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:paddingHorizontal="4dp"
                                        android:lines="2"
                                        android:gravity="center"
                                        android:text="EO Number" />

                                </TableRow>

                            </TableLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_weight=".5">
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>


                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_weight="3">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>

                        </LinearLayout>


                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <LinearLayout
                            android:orientation="horizontal"
                            android:layout_weight="2">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@drawable/cell_seasalt_bg"
                                android:gravity="center"
                                android:textSize="14sp"
                                android:text="Trace\n"
                                android:layout_weight="1"
                                android:textAlignment="center"
                                android:textStyle="bold" />
                            <TableLayout
                                android:layout_height="match_parent"
                                android:layout_width="wrap_content">
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:lines="2"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:text="Production\nDate"
                                        android:paddingVertical="4dp"
                                        android:paddingHorizontal="4dp"/>
                                </TableRow>

                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:paddingVertical="4dp"
                                        android:lines="2"
                                        android:gravity="center"
                                        android:text="Part 4M" />
                                </TableRow>
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:text="A or @"
                                        android:gravity="center"
                                        android:lines="2"
                                        android:paddingVertical="4dp"/>
                                </TableRow>
                                <TableRow>
                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="match_parent"
                                        android:textAlignment="center"
                                        android:textSize="14sp"
                                        android:textStyle="bold"
                                        android:paddingVertical="4dp"
                                        android:background="@drawable/cell_seasalt_bg"
                                        android:paddingHorizontal="4dp"
                                        android:lines="2"
                                        android:gravity="center"
                                        android:text="Trace No.\n(7~)" />

                                </TableRow>

                            </TableLayout>


                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_weight=".5">
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_height="match_parent"
                                android:layout_width="match_parent"
                                android:textAlignment="center"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>


                        </LinearLayout>

                        <LinearLayout
                            android:orientation="vertical"
                            android:layout_weight="3">
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:textAlignment="center"
                                android:layout_weight="3"
                                android:textSize="14sp"
                                android:textStyle="bold"
                                android:text=""
                                android:lines="2"
                                android:paddingVertical="4dp"
                                android:background="@drawable/cell_white_bg"/>

                        </LinearLayout>


                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        >

                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="2"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:text="Trailer"
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_seasalt_bg"/>
                        <TextView
                            android:textAlignment="center"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:text=""
                            android:layout_weight=".5"
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_white_bg"/>
                        <TextView
                            android:textAlignment="center"
                            android:layout_weight="3"
                            android:textSize="16sp"
                            android:textStyle="bold"
                            android:text=""
                            android:paddingVertical="8dp"
                            android:background="@drawable/cell_white_bg"/>

                    </TableRow>
                </TableLayout>




            </LinearLayout>

        </androidx.cardview.widget.CardView>


    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
